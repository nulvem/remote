{
    "name": "nulvem/remote",
    "description": "SCP/SSH client library for Laravel",
    "keywords": [
        "scp",
        "ssh"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Caio Kawasaki",
            "email": "caiokawasaki@gmail.com",
            "homepage": "https://github.com/caiokawasaki"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/console": "^8.0 || ^9.0 || ^10.0",
        "illuminate/support": "^8.0 || ^9.0 || ^10.0",
        "phpseclib/phpseclib": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Nulvem\\Remote\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nulvem\\Remote\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nulvem\\Remote\\RemoteServiceProvider"
            ],
            "aliases": {
                "Remote": "Nulvem\\Remote\\Facades\\Remote"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
